{
	"name": "sca-fe",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/sca-fe/src",
	"prefix": "app",
	"targets": {
		"build": {
			"executor": "@nrwl/angular:webpack-browser",
			"outputs": [
				"{options.outputPath}"
			],
			"options": {
				"customWebpackConfig": {
					"path": "apps/sca-fe/webpack.config.js"
				},
				"outputPath": "dist/apps/sca-fe",
				"index": "apps/sca-fe/src/index.html",
				"main": "apps/sca-fe/src/main.ts",
				"polyfills": [
					"zone.js"
				],
				"tsConfig": "apps/sca-fe/tsconfig.json",
				"inlineStyleLanguage": "scss",
				"allowedCommonJsDependencies": [
					"joi"
				],
				"assets": [
					"apps/sca-fe/src/favicon.ico",
					"apps/sca-fe/src/assets"
				],
				"styles": [
					"apps/sca-fe/src/styles.scss"
				],
				"scripts": []
			},
			"configurations": {
				"dev": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				},
				"qa": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				},
				"uat": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"outputHashing": "all"
				},
				"prod": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"outputHashing": "all"
				}
			},
			"defaultConfiguration": "dev"
		},
		"serve": {
			"executor": "@nrwl/angular:webpack-dev-server",
			"options": {
				"port": 4200
			},
			"configurations": {
				"dev": {
					"browserTarget": "sca-fe:build:dev"
				},
				"qa": {
					"browserTarget": "sca-fe:build:qa"
				},
				"uat": {
					"browserTarget": "sca-fe:build:uat"
				},
				"prod": {
					"browserTarget": "sca-fe:build:prod"
				}
			},
			"defaultConfiguration": "dev"
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "sca-fe:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"apps/sca-fe/**/*.ts",
					"apps/sca-fe/**/*.html"
				]
			}
		}
	},
	"tags": []
}
